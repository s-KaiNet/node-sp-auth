{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Run mocha integration",
			"type": "node",
			"request": "launch",
			"program": "${workspaceRoot}/node_modules/mocha/bin/_mocha",
			"stopOnEntry": false,
			"args": [
				"-r",
				"ts-node/register",
				"test/integration/tests.ts"
			],
			"cwd": "${workspaceRoot}",
			"protocol": "inspector"
		}
	]
}